<AnchorPane id="AnchorPane" prefHeight="404.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.LoginController">
   <children>
      <VBox layoutX="5.0" layoutY="14.0" prefHeight="337.0" prefWidth="477.0">
